         <children>
            <GridPane alignment="center" hgap="10.0" prefHeight="279.0" prefWidth="397.0" vgap="10.0">
            			<padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            			
                        			<!-- <gridLinesVisible>true</gridLinesVisible> -->
            			
                        			<Label fx:id="custDetailsLabel" prefHeight="17.0" prefWidth="123.0" text="Customer Details:" textFill="#000eff" underline="true" GridPane.columnSpan="2" />
            				
                        			<Label fx:id="custDetailsUsernameLabel" prefHeight="17.0" prefWidth="123.0" text="User Name:" GridPane.rowIndex="1" />
            			
                              			<Text fx:id="custDetailsUsernameData" wrappingWidth="124.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            			
