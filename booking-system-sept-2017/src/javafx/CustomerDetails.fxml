<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="480.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane alignment="center" hgap="10" prefHeight="480.0" prefWidth="690.0" vgap="10">
      			<padding>
            <Insets bottom="10" left="25" right="25" top="25" />
         </padding>
      			
            			<!-- <gridLinesVisible>true</gridLinesVisible> -->
      			
            			<Label fx:id="custDetailsLabel" text="Customer Details" GridPane.columnSpan="2" />
      				
            			<Label fx:id="custDetailsUsernameLabel" text="User Name:" GridPane.rowIndex="1" />
      			
            			<Text fx:id="custDetailsUsernameData" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      			
            			<Label fx:id="custDetailsFirstNameLabel" text="First Name:" GridPane.rowIndex="2" />
      			
            			<Text fx:id="custDetailsFirstNameData" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      			
            			<Label fx:id="custDetailsLastNameLabel" text="Last Name:" GridPane.rowIndex="3" />
      			
            			<Text fx:id="custDetailsLastNameData" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      			
            			<Label fx:id="custDetailsAddressLabel" text="Address:" GridPane.rowIndex="4" />
      			
            			<Text fx:id="custDetailsAddressData" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      			
            			<Label fx:id="custDetailsPhoneLabel" text="Phone Number:" GridPane.rowIndex="5" />
      			
            			<Text fx:id="custDetailsPhoneData" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
      </GridPane>
   </children>
</AnchorPane>
