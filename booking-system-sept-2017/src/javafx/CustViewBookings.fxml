<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="640.0" prefWidth="860.0" stylesheets="@customerMenu.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="custBookingsTable" layoutX="10.0" layoutY="25.0" prefHeight="401.0" prefWidth="703.0">
      	<columns>
      		<TableColumn fx:id="bookingIdColumn" prefWidth="92.0" text="Booking ID">
      			<cellValueFactory>
      				<PropertyValueFactory property="bookingId" />
              	</cellValueFactory>
              </TableColumn>
              
      		<TableColumn fx:id="dateColumn" prefWidth="104.0" text="Date">
      			<cellValueFactory>
      				<PropertyValueFactory property="date" />
      	        </cellValueFactory>
      	    </TableColumn>
      	    
      	    <TableColumn fx:id="customerNameColumn" prefWidth="160.0" text="Customer Name">
      			<cellValueFactory>
      				<PropertyValueFactory property="customerName" />
              	</cellValueFactory>
      		</TableColumn>
      		
      		<TableColumn fx:id="dayColumn" prefWidth="97.0" text="Day">
      			<cellValueFactory>
      				<PropertyValueFactory property="day" />
              	</cellValueFactory>
      		</TableColumn>
      		
      		<TableColumn fx:id="timeColumn" prefWidth="103.0" text="Time">
      			<cellValueFactory>
      				<PropertyValueFactory property="time" />
              	</cellValueFactory>
      		</TableColumn>
      		
      		<TableColumn fx:id="personalTrainerColumn" prefWidth="146.0" text="Personal Trainer">
      			<cellValueFactory>
      				<PropertyValueFactory property="employeeName" />
              	</cellValueFactory>
      		</TableColumn>
      	</columns>
      	<sortOrder>
                <fx:reference source="dateColumn" />
           </sortOrder> 
      </TableView>
   </children>
</AnchorPane>
